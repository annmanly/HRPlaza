.fountain-wishes {
    width:100%;
    height:100%;
    position:absolute;
    align-items:center;
    flex-direction:column;
    justify-content:center;

}

#fountainWishesContainer {
    width:80%;
    height:70%;
    max-width:600px;
    max-height:900px;
    border-radius:12px;
    background-color:rgba(0,0,0,1);
    color:white;
}

#_title {
    font-size:24px;
}

.hidden {
    display: none;
}